<Window x:Class="Sygnaly.Obliczenia"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Sygnaly"
        mc:Ignorable="d"
        Title="Obliczenia" Height="331.443" Width="443.814">
    <Grid Background="#FFB8B6B6">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto"/>
            <ColumnDefinition Width="Auto"/>
            <ColumnDefinition Width="Auto"/>
            <!--<ColumnDefinition Width="Auto"/>-->
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>

        <Label x:Name="Sygnal1" Grid.Row="0" Grid.Column="1" Content="Sygnal 1" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="5,5"/>
        <Label x:Name="Sygnal2" Grid.Row="0" Grid.Column="2" Content="Sygnal 2" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="5,5"/>
        <!--<Label x:Name="SygnalWynik" Grid.Row="0" Grid.Column="3" Content="Sygnal Wynikowy" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="5,5"/>-->
        <Label x:Name="Srednia" Grid.Row="1" Grid.Column="0" Content="Srednia" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="5,5"/>
        <Label x:Name="SredniaBezwzgledna" Grid.Row="2" Grid.Column="0" Content="Srednia Bezwzgledna" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="5,5"/>
        <Label x:Name="WartoscSkuteczna" Grid.Row="3" Grid.Column="0" Content="Wartosc Skuteczna" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="5,5"/>
        <Label x:Name="Wariancja" Grid.Row="4" Grid.Column="0" Content="Wariancja" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="5,5"/>
        <Label x:Name="MocSrdnia" Grid.Row="5" Grid.Column="0" Content="Srednia Moc" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="5,5"/>

        <!-- Srednie -->
        <TextBlock x:Name="SredniaSygnal1" Grid.Column="1" Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="5,5" />
        <TextBlock x:Name="SredniaSygnal2" Grid.Column="2" Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="5,5" />
        <!--<TextBlock x:Name="SredniaWynikowy" Grid.Column="3" Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="5,5" />-->

        <!-- Srednie Bezw. -->
        <TextBlock x:Name="SredniaBezwSygnal1" Grid.Column="1" Grid.Row="2" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="5,5" />
        <TextBlock x:Name="SredniaBezwSygnal2" Grid.Column="2" Grid.Row="2" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="5,5" />
        <!--<TextBlock x:Name="SredniBezwaWynikowy" Grid.Column="3" Grid.Row="2" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="5,5" />-->

        <!-- Skuteczna -->
        <TextBlock x:Name="SkutecznaSygnal1" Grid.Column="1" Grid.Row="3" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="5,5" />
        <TextBlock x:Name="SkutecznaSygnal2" Grid.Column="2" Grid.Row="3" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="5,5" />
        <!--<TextBlock x:Name="SkutecznaWynikowy" Grid.Column="3" Grid.Row="3" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="5,5" />-->

        <!-- Wariancja -->
        <TextBlock x:Name="WariancjaSygnal1" Grid.Column="1" Grid.Row="4" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="5,5" />
        <TextBlock x:Name="WariancjaSygnal2" Grid.Column="2" Grid.Row="4" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="5,5" />
        <!--<TextBlock x:Name="WariancjaWynikowy" Grid.Column="3" Grid.Row="4" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="5,5" />-->

        <!-- Moc Srednia -->
        <TextBlock x:Name="MocSredniaSygnal1" Grid.Column="1" Grid.Row="5" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="5,5" />
        <TextBlock x:Name="MocSredniaSygnal2" Grid.Column="2" Grid.Row="5" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="5,5" />
        <!--<TextBlock x:Name="MocSredniaWynikowy" Grid.Column="3" Grid.Row="5" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="5,5" />-->

    </Grid>
</Window>
